input.onButtonPressed(Button.A, () => {
    basic.showIcon(IconNames.Yes)
    basic.showIcon(IconNames.No)
    basic.showIcon(IconNames.Yes)
    basic.showIcon(IconNames.No)
    if (Math.randomBoolean()) {
        basic.showIcon(IconNames.Yes)
    } else {
        basic.showIcon(IconNames.No)
    }
})
basic.forever(() => {
	
})
